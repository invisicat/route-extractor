  "devDependencies": {
    "@types/bun": "latest",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/babel__traverse": "^7.28.0",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
